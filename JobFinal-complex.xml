<!-- JobFinal.xml - A more complex example -->
<Root>
	<Job>
		<Client>
			<ID>Classic</ID>
			<DisplayName>Ruslan Piskarev</DisplayName>
		</Client>
		<Production>
			<WorkStatement>\WorkStatements\M456.xls</WorkStatement>
			<InvoiceItems>
				<Product>
					<ID>"Prep_Ind"</ID>
					<Description>Individual-Image Prep</Description>
					<!-- counts come from order information -->
					<IndCount>15</IndCount>
					<!-- price is found in catalog -->
					<PricePerInd>.45</PricePerInd>
				</Product>
				<Product>
					<ID>"Prep_AI"</ID>
					<IndCount>15</IndCount>
					<Description>Associated-Image Prep</Description>
					<PricePerInd>.85</PricePerInd>
				</Product>
				<Product>
					<ID>"Prod_SPP"</ID>
					<Description>Single Pose Portrait production</Description>
					<RenderCount>20</RenderCount>
					<PricePerRender>.50</PricePerRender>
				</Product>
				<Product>
					<ID>"Prod_SMM"</ID>
					<Description>Static Memory Mate production</Description>
					<PricePerRender>0.55</PricePerRender>
					<RenderCount>13</RenderCount>
				</Product>
				<Product>
					<ID>"Prod_RC"</ID>
					<Description>Rotated Composite production</Description>
					<PricePerRender>0.65</PricePerRender>
					<RenderCount>5</RenderCount>
				</Product>
				<Product>
					<ID>"Overage-PP"</ID>
					<Description>Extra time required for production - prepaid</Description>
					<PricePerTick>.22</PricePerTick>
					<!-- count comes from production -->
					<TickCount>15</TickCount>
				</Product>
				<Product>
					<ID>"Prod_RS"</ID>
					<Description>Rendered Sheets</Description>
					<PricePerSheet>.05</PricePerSheet>
					<!-- count comes from production -->
					<SheetCount>18</SheetCount>
				</Product>
			</InvoiceItems>
			<Responses>
				<!-- Comments come from production -->
				<CommentsToTheClient>Please check spelling of student name on memory mate 6</CommentsToTheClient>
			</Responses>
		</Production>
	</Job>
	<Meta>
		<DBVersion>20151207</DBVersion>
	</Meta>
</Root>
